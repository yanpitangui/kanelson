@inject IStringLocalizer<Shared> Loc

<MudNavMenu>
    <MudNavLink Href="" Match="NavLinkMatch.All" StartIcon="@Icons.Material.Filled.Home">@Loc["InitialPage"]</MudNavLink>
    <AuthorizeView>
        <Authorized>
            <MudNavLink Href="questions" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.QuestionMark">@Loc["Questions"]</MudNavLink>
            <MudNavLink Href="templates" Match="NavLinkMatch.Prefix" Icon="@Icons.Custom.Uncategorized.FolderOpen">@Loc["Templates"]</MudNavLink>
            <MudNavLink Href="rooms" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.PlayArrow">@Loc["Rooms"]</MudNavLink>
        </Authorized>
    </AuthorizeView>

</MudNavMenu>